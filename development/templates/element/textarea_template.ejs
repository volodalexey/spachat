<% var _in = arguments[0]; %>
<textarea class="<%=_in.config.class%>"

        <% if(_in.config.data) {
                for (var dataKey in _in.config.data) {
        if (_in.config.data[dataKey] !== "") { %>
        <%= 'data-' + dataKey + '=\"' + _in.config.data[dataKey] + '\"' %>
        <% }
        }
        } %>

<% if(_in.config.rows){ %> rows="<%= _in.config.rows%>" <%}%>
<% if(_in.config.value !== ""){ %> value="<%=_in.config.value%>" <%}%>

><%=_in.config.text%></textarea>
