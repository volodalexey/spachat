<% if(_in.config.element === "button") { %>
<%= _in.button_template(_in) %>
<% } %>

<% if(_in.config.element === "label") { %>
<%= _in.label_template(_in) %>
<% } %>

<% if(_in.config.element === "input") { %>
<%= _in.input_template(_in) %>
<% } %>

<% if(_in.config.element === "textarea") { %>
<%= _in.textarea_template(_in) %>
<% } %>

<% if(_in.config.element === "select") { %>
<%= _in.select_template(_in) %>
<% } %>

<% if(_in.config.element === "svg") { %>
    <img src="templates/icon/<%= _in.config.icon %>" class="transition-all" data-role="pointer">
<% } %>

